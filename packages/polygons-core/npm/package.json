{
  "name": "polygon-editor",
  "version": "0.0.22",
  "author": "Stephen Fraser <stephen.fraser@digirati.com>",
  "description": "A library for editing polygons, polylines and points",
  "type": "module",
  "module": "./index.js",
  "main": "./index.cjs",
  "files": [
    "index.d.cts",
    "index.d.ts",
    "index.js",
    "index.cjs"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "scripts": {
    "prepack": "tsup"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "contours.ts": "^0.2.2",
    "tsup": "^8.5.0"
  }
}
